{
	"name": "eslint-plugin-arrow-return-style-x",
	"version": "1.0.0-alpha.0",
	"description": "Enforce arrow function return style and automatically fix it",
	"keywords": ["arrow-body-style", "arrow-return-style", "arrow-function"],
	"repository": {
		"url": "https://github.com/christopher-buss/eslint-plugin-arrow-return-style-x.git",
		"type": "git"
	},
	"license": "MIT",
	"author": "Christopher Buss <christopher.buss@pm.me> (https://github.com/christopher-buss)",
	"contributors": ["u3u <qwq@qwq.cat> (https://github.com/u3u)"],
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": ["./dist/*", "./dist/index.d.ts"]
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "nr generate:types && tsdown --clean --dts",
		"dev": "tsdown --stub",
		"eslint-docs": "eslint-doc-generator",
		"generate:types": "tsx scripts/generate-types.ts",
		"lint": "eslint --cache",
		"prepare": "simple-git-hooks",
		"prepublishOnly": "nr build",
		"release": "bumpp",
		"start": "tsx src/index.ts",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged --concurrent false"
	},
	"lint-staged": {
		"*.{js,cjs,mjs,jsx,ts,tsx,md}": ["eslint --fix"],
		"*": ["prettier --write --ignore-unknown"]
	},
	"dependencies": {
		"@typescript-eslint/type-utils": "catalog:cli",
		"@typescript-eslint/utils": "catalog:cli"
	},
	"devDependencies": {
		"@antfu/ni": "catalog:cli",
		"@isentinel/eslint-config": "catalog:cli",
		"@types/node": "catalog:types",
		"@typescript-eslint/eslint-plugin": "catalog:cli",
		"@typescript-eslint/parser": "catalog:cli",
		"@vitest/eslint-plugin": "catalog:",
		"bumpp": "catalog:cli",
		"eslint": "catalog:cli",
		"eslint-doc-generator": "catalog:utils",
		"eslint-plugin-eslint-plugin": "catalog:testing",
		"eslint-plugin-pnpm": "catalog:cli",
		"eslint-vitest-rule-tester": "catalog:testing",
		"jiti": "catalog:cli",
		"lint-staged": "catalog:cli",
		"publint": "catalog:testing",
		"simple-git-hooks": "catalog:cli",
		"ts-api-utils": "catalog:cli",
		"tsdown": "catalog:cli",
		"tsx": "catalog:cli",
		"typescript": "catalog:cli",
		"unplugin-unused": "catalog:testing",
		"vitest": "catalog:testing"
	},
	"peerDependencies": {
		"eslint": ">=9.10.0"
	},
	"packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
	"engines": {
		"node": ">=20.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
